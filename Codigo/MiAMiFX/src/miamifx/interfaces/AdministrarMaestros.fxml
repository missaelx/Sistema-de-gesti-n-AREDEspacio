            <TableView fx:id="tablaMaestros" layoutX="14.0" layoutY="79.0" prefHeight="419.0" prefWidth="696.0">
               <columns>
                  <TableColumn fx:id="columnaNombre" prefWidth="200.0" text="Nombre" />
